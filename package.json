{
  "name": "reax",
  "version": "1.0.0",
  "description": "an insomorphic react app with redux, rr and styled-components",
  "main": "src/server/dev.js",
  "repository": "https://github.com/ahmadaidil/reax.git",
  "author": "ahmadaidil <ahmdaidil@gmail.com>",
  "license": "MIT",
  "scripts": {
    "prestart": "webpack --config webpack/webpack.config.ssr.js",
    "start": "nodemon .",
    "prestart:prod": "npm run build && npm run build:ssr",
    "start:prod": "NODE_ENV=production node ./build/server.js",
    "build": "NODE_ENV=production webpack --config webpack/webpack.config.js",
    "build:ssr": "NODE_ENV=production webpack --config webpack/webpack.config.ssr.js",
    "lint": "esw --ext .jsx --ext .js --color",
    "lint:css": "stylelint './src/**/*.js'",
    "precommit": "echo 'Linter checks...' && exit 0"
  },
  "pre-commit": [
    "precommit",
    "lint",
    "lint:css"
  ],
  "dependencies": {
    "express": "^4.16.4",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-redux": "^7.0.1",
    "react-router": "^5.0.0",
    "redux": "^4.0.1",
    "styled-components": "^4.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.3",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-destructuring": "^7.4.3",
    "@babel/plugin-transform-react-constant-elements": "^7.2.0",
    "@babel/preset-env": "^7.4.3",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "babel-plugin-styled-components": "^1.10.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "copy-webpack-plugin": "^5.0.2",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.12.4",
    "eslint-plugin-react-hooks": "^1.6.0",
    "eslint-watch": "^5.0.1",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.6.0",
    "nodemon": "^1.18.11",
    "pre-commit": "^1.2.2",
    "stylelint": "^9.10.1",
    "stylelint-config-recommended": "^2.1.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.6.0",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.3.0",
    "webpack-dev-middleware": "^3.6.2",
    "webpack-hot-middleware": "^2.24.3"
  }
}
